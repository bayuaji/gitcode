@model GitCode.Models.Comment

@{
    ViewBag.Title = "Delete";
}

<h2>Delete</h2>

<h3>Are you sure you want to delete this comment?</h3>
@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    <p>
        <input type="submit" value="Delete" /> | @Html.ActionLink("Back", "Details", "issue", new { id = Model.IssueId }, null)
    </p>
}
